            <div class="col-md-12">
                <div >
                    <div class="row">

                        <div class="col-md-3 go_to">
                            <div class="dash-box card  dash-box-color-2" style="margin-top: 29px;">
                                <div class="card-body">
                                    <div class="stat-widget-one">
                                        <h2><div style="color: #fff;" class="stat-icon-wp dib"><i class="fa fa-fw fa-calendar-plus-o"/></div></h2>

                                        <div class="stat-content dib">
                                            <h3><span style="color: #fff;" class="dash-box-title">Create Meeting</span></h3>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>

                        
                        <div class="col-md-3 meeting_count">
                            <div class="dash-box card dash-box-color-3" style="margin: 30px 0 50px;">
                              <a href="javascript:;" class="dashboard-block ">
                                <div class="rotate">
                                  <i class="fa fa-calendar"></i>
                                </div>
                                <div class="details">
                                  <!-- <span class="title">Payslips</span> -->
                                  <h3><p style="color: #fff;" class="dash-box-title">Todays Meetings</p></h3>
                                  <h2><p style="color: #fff;" class="stat-count"><t t-esc="widget.login_user.meetings_count"/></p></h2>
                                  
                                </div><!--/details-->
                                <i class="fa fa-chevron-right fa-2x more"></i>
                              </a><!--/dashboard-block2-->
                            </div>
                        </div>

                        <div class="col-md-3 meeting_count_month">
                            <div class="dash-box card dash-box-color-6" style="margin: 30px 0 50px;">
                              <a href="javascript:;" class="dashboard-block ">
                                <div class="rotate">
                                  <i class="fa fa-calendar-check-o"></i>
                                </div>
                                <div class="details">
                                  <!-- <span class="title">Payslips</span> -->
                                  <h3><p style="color: #fff;" class="dash-box-title">This Month</p></h3>
                                  <h2><p style="color: #fff;" class="stat-count"><t t-esc="widget.login_user.meetings_count_month"/></p></h2>
                                  

                                </div><!--/details-->
                                <i class="fa fa-chevron-right fa-2x more"></i>
                              </a><!--/dashboard-block2-->
                            </div>
                        </div>


                        <div class="col-md-3 draft_meeting_count_month">

                            <div class="dash-box card dash-box-color-1" style="margin: 30px 0 50px;">
                              <a href="javascript:;" class="dashboard-block ">
                                <div class="rotate">
                                  <i class="fa fa-calendar-times-o"></i>
                                </div>
                                <div class="details">
                                  <!-- <span class="title">Payslips</span> -->
                                  <h3><p style="color: #fff;" class="dash-box-title">Draft Meetings</p></h3>
                                  <h2><p style="color: #fff;" class="stat-count"><t t-esc="widget.login_user.draft_meetings_count_month"/></p></h2>
                                  
                                </div><!--/details-->
                                <i class="fa fa-chevron-right fa-2x more"></i>
                              </a><!--/dashboard-block2-->
                            </div>
                        </div>










